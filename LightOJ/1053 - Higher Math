#include<iostream>
#include<cstdio>
#include<cmath>
#include<cstring>
#include<algorithm>
#include<vector>
#include<map>
using namespace std;
#define pf printf
#define sf scanf
#define loop(i, n) for(i=0; i<(n); i++)
#define MAX 100

int main()
{
    int test, t;
    int a, b, c;
    sf("%d", &test);
    for(t=1; t<=test; t++)
    {
        sf("%d %d %d", &a, &b, &c);

        if(((a*a)+(b*b))==(c*c) || ((b*b)+(c*c))==(a*a) || ((a*a)+(c*c))==(b*b))
            pf("Case %d: yes\n", t);
        else
            pf("Case %d: no\n", t);
    }
    return 0;
}
