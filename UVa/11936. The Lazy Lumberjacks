#include<iostream>
#include<cstdio>
#include<cmath>
#include<cstring>
#include<algorithm>
using namespace std;
#define ll long long
#define sf scanf
#define pf printf
#define PI 2*acos(0.0)

int main()
{
   int t;
   sf("%d",&t);
   while(t--)
   {
       ll a, b, c;
       sf("%lld%lld%lld", &a, &b, &c);

       if(a+b>c && b+c>a && a+c>b)
            pf("OK\n");
       else
            pf("Wrong!!\n");
   }
   return 0;
}
